def cria_novo_veiculo():
    tipo = input('Tipo(Carro/Mota): ')
    marca = input('Marca: ')
    modelo = input('Modelo: ')
    matricula = input('Matrícula: ')
    cor = input('Cor: ')
    comprimento = input('Comprimento: ')
    cilindrada = input('Cilindrada: ')

    veiculo = (f'Tipo: {tipo}\n Marca: {marca}\n Modelo: {modelo}\n Matrícula: {matricula}\n Cor: {cor}\n Comprimento: {comprimento}\n Cilindrada: {cilindrada}')

    return veiculo
